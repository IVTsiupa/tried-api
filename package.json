{
  "name": "tried-api",
  "version": "1.0.0",
  "description": "API for Tried service",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "cross-env NODE_ENV=development nodemon",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "migration:run": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IVTsiupa/tried-api.git"
  },
  "author": "Ivan Tsiupa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IVTsiupa/tried-api/issues"
  },
  "homepage": "https://github.com/IVTsiupa/tried-api#readme",
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "fastify": "^3.27.4",
    "uuid": "^8.3.2"
  }
